{
  "schemaVersion": 1,
  "id": "impaled",
  "version": "${version}",
  "name": "Impaled",
  "description": "Adds trident variants and a Sincere Loyalty enchantment upgrade for tridents.",
  "authors": [
    "doctor4t",
    "ArathainFarqoe",
    "Pyrofab"
  ],
  "contact": {
    "sources": "https://github.com/Ladysnake/Impaled",
    "issues": "https://github.com/Ladysnake/Impaled/issues"
  },
  "license": "ARR",
  "icon": "assets/impaled/impaled.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "org.ladysnake.impaled.common.Impaled",
      "org.ladysnake.sincereloyalty.SincereLoyalty"
    ],
    "client": [
      "org.ladysnake.impaled.client.ImpaledClient",
      "org.ladysnake.sincereloyalty.SincereLoyaltyClient::INSTANCE"
    ],
    "emi": [
      "org.ladysnake.impaled.compat.EMICompat"
    ]
  },
  "mixins": [
    "impaled.mixins.json",
    "sincereloyalty.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.15.0",
    "java": ">=17",
    "fabric": ">=0.48.0"
  },
  "custom": {
    "loom:injected_interfaces": {
      "net/minecraft/class_8109": [
        "org/ladysnake/impaled/common/damage/DamageSourcesExt"
      ]
    },
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.ladysnake.org",
        "modmenu.wiki": "https://ladysnake.org/wiki/impaled"
      }
    }
  }
}
